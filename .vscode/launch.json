    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'fsm'",
            "cargo": {
                "args": [
                    "run",
                    "--bin=fsm",
                    "--package=fsm",
                ],
                "filter": {
                    "name": "fsm",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "Build Debug Target"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'fsm'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=fsm",
                    "--package=fsm"
                ],
                "filter": {
                    "name": "fsm",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "Build Debug Target"
        }
    ]
}
